<div class="row">
  <% @categories.each do |category| %>
    <div class="col-xs-6 col-md-3">
      <a href=<%= kids_levels_path %>>
        <div class="thumbnail big-btn">
          <span class="level-label"><%= category.name %></span>
        </div>
      </a>
    </div>
  <% end %>
</div>
<div class="row">
  <div class="col-md-12">
    <%= link_to("Go next >", kids_levels_path, class: "btn btn-default btn-lg pull-right") %>
  </div>
</div>